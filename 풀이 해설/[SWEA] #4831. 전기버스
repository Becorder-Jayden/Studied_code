#### í’€ì´ðŸ›«

T = int(input())
for t in range(T):
    K, N, M = map(int, input().split())
    station = [0] + list(map(int, input().split())) + [10]

    curIdx = 0    # í˜„ìž¬ì˜ ìœ„ì¹˜
    charCnt = 0  # ì¶©ì „ íšŸìˆ˜
    
    for i in range(1, M+2):
    
        # ëª©ì ì§€ì— ë„ë‹¬í•  ìˆ˜ ì—†ëŠ” ê²½ìš°
        if station[i] - station[i-1] > K:
            answer = 0
            break
    
        if curIdx + K < station[i]:
            curIdx = station[i]
            charCnt += 1
    
    print(f"#{t+1}", charCnt)

