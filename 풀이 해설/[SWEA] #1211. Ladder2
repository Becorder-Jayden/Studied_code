#### í’€ì´ğŸ›«

T = 10

for t in range(T):
    testcase = int(input())
    arr = [list(map(int, input().split())) for i in range(100)]
    # ì¶œë°œ ê°€ëŠ¥í•œ ì¸ë±ìŠ¤ë¥¼ êµ¬í•¨    
    startList = []
    for i in range(100):
        if arr[0][i] == 1:
            startList.append(i)

    def cntRoute(startRow):
        col, row = 0, startRow
        cntSide = 0 
        while col != 99:
            # ì™¼ìª½ì— ê¸¸ì´ ìˆì„ ë•Œ
            if 0<=row-1 and arr[col][row-1] == 1:
                while 0<=row-1 and arr[col][row-1] == 1:
                    row -= 1
                    cntSide += 1
    
            # ì˜¤ë¥¸ìª½ì— ê¸¸ì´ ìˆì„ ë•Œ
            elif 100>row+1 and arr[col][row+1] == 1:
                while 100>row+1 and arr[col][row+1] == 1:
                    row += 1
                    cntSide += 1
    
            # ì™¼ìª½, ì˜¤ë¥¸ìª½ì— ê¸¸ì´ ì—†ì„ ë•Œ ì•„ë˜ë¡œ ì§„í–‰
            col += 1
    
        return cntSide
    # ì¶”ê°€ ì´ë™í•œ ë§Œí¼ì˜ ì–‘ì„ ë‹´ëŠ” ë¦¬ìŠ¤íŠ¸ ìƒì„±
    cntSideList = []
    for i in startList:
        cntSideList.append(cntRoute(i))
    
    # cntSideListì—ì„œ ê°€ì¥ ì‘ì€ ê°’ì„ ê°–ëŠ” ì¸ë±ìŠ¤ì˜ ì‹œì‘ì  ì¶œë ¥
    answer = startList[cntSideList.index(min(cntSideList))]
    print(f"#{testcase}", answer)

