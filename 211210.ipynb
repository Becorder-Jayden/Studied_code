   "source": [
    "a = input().split('-')\n",
    "li = []\n",
    "\n",
    "for i in a:\n",
    "    cnt = 0\n",
    "    i = i.split('+')\n",
    "    for j in i:\n",
    "        cnt += int(j)\n",
    "    li.append(cnt)\n",
    "\n",
    "n = li[0]\n",
    "for i in range(1, len(li)):\n",
    "    n -= li[i]\n",
    "print(n)"
   ]
